
public class quadraticEquation{
    private int a;
    private int b;
    private int c;

    quadraticEquation(int newA, int newB, int newC){
        a = newA;
        b = newB;
        c = newC;
    }


    //getter
    public int getA(){
        return a;
    }

    public int getB(){
        return b;
    }

    public int getC(){
        return c;
    }


    public double getDiscriminant(){
       return (Math.pow(b, 2) - 4*a*c);
    }

}
